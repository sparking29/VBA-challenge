Sub Homework()'Loop through all worksheetsFor Each ws In Worksheets'Add Column Namesws.Cells(1, 9).Value = "Ticker"ws.Cells(1, 10).Value = "Yearly Change"ws.Cells(1, 11).Value = "Percent Change"ws.Cells(1, 12).Value = "Total Stock Volume"ws.Cells(1, 14).Value = "Greatest % Increase"ws.Cells(1, 15).Value = "Greatest % Decrease"ws.Cells(1, 16).Value = "Greatest Total Volume"Dim Ticker As StringDim YearChange As DoubleDim YearOpen As DoubleDim YearClose As DoubleDim PercentChange As DoubleDim StockVol As DoubleDim Count As LongStockVol = 0Count = 2On Error Resume Next'Loop through all stocksFor i = 2 To ws.Cells(1, 1).End(xlDown).Row    'Check if we are in the same Stock    If ws.Cells(i + 1, 1).Value <> ws.Cells(i, 1).Value Then            'Set Stock        Ticker = ws.Cells(i, 1).Value            'Add to Stock Volume        StockVol = StockVol + ws.Cells(i, 7).Value                'Calculate YearChange        YearOpen = ws.Range("C" & Count)        YearClose = ws.Range("F" & i)        YearChange = YearClose - YearOpen                'Calculate Percent Change        PercentChange = YearChange / YearOpen            'Print Stock        ws.Range("I" & Count).Value = Ticker            'Print Volume        ws.Range("L" & Count).Value = StockVol                'Print Yearly Change        ws.Range("J" & Count).Value = YearChange                'Print Percent Change        ws.Range("K" & Count).Value = PercentChange            'Add to Count        Count = Count + 1            'Reset All        StockVol = 0        PercentChange = 0            End If        'Format Cell Color    If ws.Cells(i, 10) >= 0 Then    ws.Cells(i, 10).Interior.ColorIndex = 4        Else:    ws.Cells(i, 10).Interior.ColorIndex = 3        End If        'Format Percent    ws.Range("K2:K3000").NumberFormat = "0.00%"    Next i    Next wsEnd Sub